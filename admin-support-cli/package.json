  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "main": "index.js",
  "scripts": {
    "prepare": "cd .. && husky install ./.github/husky",
    "prebuild": "rimraf dist",
    "build": "ncc build src/action.js --out dist --license licenses.txt --quiet",
    "format": "standard --fix",
    "start": "node src/cli.js",
    "test": "standard && jest --coverage"
  },
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "@octokit/graphql": "^4.8.0",
    "@octokit/plugin-retry": "^3.0.9",
    "@octokit/plugin-throttling": "^3.5.2",
    "commander": "^9.0.0",
    "@octokit/rest": "^18.12.0",
    "node-notifier": ">=10.0.1",
    "validate.js": "^0.13.1",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.33.3",
    "eslint": "^8.9.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "nock": "^13.2.4",
    "rimraf": "^3.0.2",
    "standard": "^16.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./.github/husky/pre-commit",
      "pre-push": "./.github/husky/pre-push"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/?(*.)+(spec|test).[j]s?(x)"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": [
      "dist/"
    ]
  }
}
